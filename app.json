{
  "expo": {
    "name": "centrix_client",
    "slug": "centrix_client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android/drawable-xxhdpi/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#202871"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/drawable-xxxhdpi/logo.png",
        "backgroundColor": "#202871"
      }
    },
    "web": {
      "favicon": "./assets/android/drawable-xxxhdpi/logo.png"
    },
    "plugins": ["expo-font"]
  }
}
